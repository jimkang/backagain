{
  "manifest_version": 2,

  "name": "Back Again?",
  "description": "Shows the number of times you've visited a web site in a day.",
  "version": "1.1.1",

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "history"
  ],

  "background": {
    "scripts": [
      "underscore.js",
      "d3.v3.min.js",
      "makeicon.js",
      "colordesignator.js",
      "historyfilter.js",
      "reporter.js",
      "graph.js",
      "popupreporter.js",
      "background.js"
    ],
    "persistent": false
  },

  "browser_action": {
    "default_icon": {
      "19": "backagain_icon_19.png",
      "38": "backagain_icon_38.png"
    },
    "default_popup": "graphs.html"
  },

  "icons": {
    "16": "backagain_icon_16.png",
    "48": "backagain_icon_48.png",
    "128": "backagain_icon_128.png"
  }
}

